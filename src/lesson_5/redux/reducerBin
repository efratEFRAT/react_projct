import { ADD_TODO, REMOVE_TODO } from './actionTyps';
const initalstate = { taskBin:[] };

 const reducerBin = (state = initalstate, action) => {
    switch (action.type) {
        case REMOVE_TODO:
            return { ...state,taskBin:[...state.taskBin, action.payload], };
        default:
            return state;
    }
};
export default reducerBin; 